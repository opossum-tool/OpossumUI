{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/index.tsx", "src/ElectronBackend/{app,preload}.ts"],
  "project": ["**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}!"],
  "ignore": ["src/ElectronBackend/db/generated/*"],
  "ignoreDependencies": ["dpdm", "better-sqlite3-electron"],
  "ignoreBinaries": [
    "tools/*",
    "spdx",
    "reuse",
    "licenses",
    "codesign",
    "ship-mac"
  ]
}
