{
  "name": "opossum-ui",
  "productName": "OpossumUI",
  "description": "The OpossumUI enables the editing of attribution information that is assigned to a resource tree.",
  "license": "Apache-2.0",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource-variable/karla": "^5.2.8",
    "@mui/icons-material": "^7.3.4",
    "@mui/material": "^7.3.4",
    "@mui/system": "^7.3.3",
    "@mui/utils": "^7.3.3",
    "@reduxjs/toolkit": "^2.9.0",
    "@tanstack/react-query": "^5.90.2",
    "chroma-js": "^3.1.2",
    "compare-versions": "^6.1.1",
    "dayjs": "^1.11.18",
    "electron-log": "^5.4.3",
    "electron-settings": "^4.0.4",
    "fast-csv": "^5.0.5",
    "fflate": "^0.8.2",
    "js-yaml": "^4.1.0",
    "jsonschema": "^1.5.0",
    "lodash": "^4.17.21",
    "object-hash": "^3.0.0",
    "packageurl-js": "^2.0.1",
    "proxy-memoize": "^3.0.1",
    "re-resizable": "^6.11.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hot-toast": "^2.6.0",
    "react-hotkeys-hook": "^5.2.0",
    "react-redux": "^9.2.0",
    "react-virtuoso": "4.14.1",
    "recharts": "^3.2.1",
    "spdx-license-ids": "^3.0.22",
    "stream-json": "^1.9.1",
    "upath": "^2.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.37.0",
    "@faker-js/faker": "^10.0.0",
    "@playwright/test": "^1.56.0",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/babel__core": "^7.20.5",
    "@types/chroma-js": "^3.1.1",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.18.10",
    "@types/object-hash": "^3.0.6",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/stream-json": "^1.7.8",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-transform-import-meta": "^2.3.3",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "dpdm": "^3.14.0",
    "electron": "38.2.2",
    "electron-builder": "^26.0.12",
    "electron-playwright-helpers": "^1.8.2",
    "eslint": "^9.37.0",
    "eslint-plugin-filenames-simple": "^0.9.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-testing-library": "^7.13.2",
    "globals": "^16.4.0",
    "husky": "9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-watch-typeahead": "^3.0.1",
    "knip": "^5.65.0",
    "lint-staged": "^16.2.4",
    "mkdirp": "^3.0.1",
    "mockdate": "^3.0.5",
    "nunjucks": "^3.2.4",
    "prettier": "^3.6.2",
    "prettier-plugin-sh": "^0.18.0",
    "run-script-os": "^1.1.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.9",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4",
    "whatwg-fetch": "^3.6.20"
  },
  "scripts": {
    "start": "run-script-os",
    "start:darwin:linux": "tools/start_linux_darwin.sh",
    "start:win32": "tools/start_windows.bat",
    "build": "yarn generate-notice && vite build && tsc -p src/ElectronBackend",
    "circular-import-check": "find src/ | xargs dpdm --circular -T --tree false --warning false --exit-code circular:1",
    "typecheck": "tsc -p ./ && tsc --noEmit -p src/ElectronBackend",
    "test:unit": "jest",
    "test:changed": "jest --onlyChanged",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test -c src/e2e-tests/playwright.config.ts",
    "test:e2e:ui": "playwright test -c src/e2e-tests/playwright.config.ts --ui",
    "test:e2e:ci": "yarn playwright test -c src/e2e-tests/playwright.config.ts",
    "test:prepare": "NODE_ENV=e2e tsc -p src/ElectronBackend && vite -m e2e",
    "lint": "eslint --fix",
    "lint-check": "eslint",
    "lint-commits": "commitlint -f ${TARGET_BRANCH:-origin/main} --verbose",
    "format": "prettier --write --ignore-unknown .",
    "format-check": "prettier --check --ignore-unknown .",
    "copyright-lint-check": "reuse lint",
    "knip": "knip",
    "update-commit-hash": "run-script-os",
    "update-commit-hash:darwin:linux": "COMMIT_INFO=$(git describe --exact-match --tags 2> /dev/null || git rev-parse --short HEAD); echo \"{\\\"commitInfo\\\" : \\\"$COMMIT_INFO\\\" }\" > \"src/commitInfo.json\"",
    "update-commit-hash:win32": "tools/get_app_version_for_windows.bat",
    "generate-notice": "mkdirp notices && yarn licenses generate-disclaimer --production > notices/notices.txt && yarn node tools/generateNotices.mjs && ./tools/checkNotices.sh",
    "ship-linux": "yarn install-deps:linux && yarn build && tools/build_linux_release.sh",
    "ship-win": "yarn install-deps:win32 && yarn build && electron-builder --win --x64 --publish never && mkdirp release/win && mv \"release/OpossumUI Setup 0.1.0.exe\" \"release/win/OpossumUI-for-win.exe\"",
    "ship-mac:x64": "yarn install-deps:darwin:x64 && yarn build && electron-builder --mac --x64 --publish never && cd release/mac && zip -r -y -q 'OpossumUI-for-mac-intel.zip' 'OpossumUI.app'",
    "ship-mac:arm64": "yarn install-deps:darwin:arm64 && yarn build && electron-builder --mac --arm64 --publish never && codesign --force --deep -s - release/mac-arm64/OpossumUI.app && cd release/mac-arm64 && zip -r -y -q 'OpossumUI-for-mac-arm64.zip' 'OpossumUI.app'",
    "ship": "yarn ship-linux && yarn ship-win && yarn ship-mac",
    "ship:auto": "run-script-os",
    "ship:auto:darwin": "yarn node tools/runScriptArch.mjs ship-mac",
    "ship:auto:linux": "yarn ship-linux",
    "ship:auto:win32": "yarn ship-win",
    "clean": "rm -rf ./build/ ./release/",
    "postinstall": "yarn install-deps && husky && yarn update-commit-hash",
    "pre-commit": "lint-staged",
    "install-deps": "run-script-os",
    "install-deps:darwin:x64": "yarn node --env-file-if-exists=.env tools/downloadOpossumFile.mjs mac-intel",
    "install-deps:darwin:arm64": "yarn node --env-file-if-exists=.env tools/downloadOpossumFile.mjs mac-arm64",
    "install-deps:linux": "yarn node --env-file-if-exists=.env tools/downloadOpossumFile.mjs linux",
    "install-deps:win32": "yarn node --env-file-if-exists=.env tools/downloadOpossumFile.mjs windows.exe"
  },
  "main": "build/ElectronBackend/app.js",
  "build": {
    "productName": "OpossumUI",
    "appId": "opossum_ui_electron",
    "files": [
      "build/**/*",
      "bin/*",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!build/**/__tests__/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "mac": {
      "target": "dmg",
      "icon": "public/icons/icon_512x512.png"
    },
    "win": {
      "target": "nsis",
      "icon": "public/icons/icon_512x512.png"
    },
    "linux": {
      "target": [
        "snap",
        "AppImage"
      ],
      "icon": "public/icons/icon_512x512.png",
      "category": "Development"
    },
    "extraMetadata": {
      "main": "build/ElectronBackend/app.js"
    },
    "extraResources": [
      "./notices/**"
    ]
  },
  "browserslist": {
    "production": [
      "Chrome 109"
    ],
    "development": [
      "Chrome 109"
    ]
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "yarn@4.10.3"
}
