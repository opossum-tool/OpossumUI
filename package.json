    "react-error-boundary": "^4.0.13",
    "react-hot-toast": "^2.4.1",
    "react-hotkeys-hook": "^4.5.0",
    "react-redux": "^9.1.2",
    "react-virtuoso": "4.7.6",
    "recharts": "^2.12.7",
